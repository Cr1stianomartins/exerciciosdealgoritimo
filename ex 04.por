programa {
  inteiro numero, antecessor, sucessor
  funcao inicio() {
    escreva("Digite um numero inteiro: ")
    leia(numero)
    antecessor = numero - 1
    sucessor = numero + 1


     escreva("O antecessor de ", numero, "é: ", antecessor
     escreva("O sucessor de ", numero, "é: ", sucessor, )
}
}